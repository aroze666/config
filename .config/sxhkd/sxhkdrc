  scrot -s /home/aroze/Pictures/screenshots/'%Y-%m-%d %H:%M:%S $wx$h.png'

super + s
  scrot /home/aroze/Pictures/screenshots/'%Y-%m-%d %H:%M:%S $wx$h.png'

